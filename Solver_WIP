import numpy as np




def parse(map = 'Sokoban_map.txt'):
  ### Line Parser ###
  # Using readlines() 
  map_txt = open(map, 'r') 
  Lines = map_txt.readlines() 
  
  # Strips the newline character 
  for line in Lines: 
    line = line.strip("\n")
  
  map_arr = np.array(Lines)



